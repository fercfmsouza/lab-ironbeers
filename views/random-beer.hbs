<div id="random-beer" class='beers-list'>
  {{#each responseFromAPI}}
  <div>
    {{> beerpartial}}
    <h2>Food Pairing</h2>
    <ul>
      {{#each this.food_pairing}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p><strong>Brewers tips:</strong> {{this.brewers_tips}}</p> 
  </div>
  {{/each}}
</div>